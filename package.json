{
  "name": "leptos-motion",
  "version": "0.8.3",
  "description": "A comprehensive animation library for Rust and Leptos",
  "private": true,
  "scripts": {
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "install:browsers": "playwright install",
    "install:browsers:chromium": "playwright install chromium",
    "install:browsers:firefox": "playwright install firefox",
    "install:browsers:webkit": "playwright install webkit",
    "dev": "cd examples/showcase && trunk serve",
    "dev:watch": "cd examples/showcase && trunk serve --watch",
    "build:examples": "cd examples/showcase && trunk build",
    "build:examples:release": "cd examples/showcase && trunk build --release",
    "clean": "cargo clean && rm -rf dist/ target/ node_modules/",
    "clean:all": "pnpm clean && pnpm store prune",
    "lint": "cargo clippy --all-targets --all-features -- -D warnings",
    "lint:fix": "cargo clippy --all-targets --all-features --fix --allow-dirty --allow-staged",
    "format": "cargo fmt --all",
    "format:check": "cargo fmt --all -- --check",
    "test": "cargo test --all",
    "test:coverage": "cargo tarpaulin --out Html --output-dir coverage",
    "test:integration": "cargo test --test '*' --features integration",
    "bench": "cargo bench",
    "check": "cargo check --all-targets --all-features",
    "build": "cargo build --all-features",
    "build:release": "cargo build --release --all-features",
    "docs": "cargo doc --all-features --open",
    "docs:build": "cargo doc --all-features --no-deps",
    "preinstall": "pnpm store prune",
    "postinstall": "playwright install --with-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloud-shuttle/leptos-motion.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/cloud-shuttle/leptos-motion/issues"
  },
  "homepage": "https://github.com/cloud-shuttle/leptos-motion#readme"
}
