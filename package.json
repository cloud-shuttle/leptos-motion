{
  "name": "leptos-motion",
  "version": "0.1.0-alpha",
  "description": "A comprehensive animation library for Rust and Leptos",
  "private": true,
  "scripts": {
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "install:browsers": "playwright install",
    "install:browsers:chromium": "playwright install chromium",
    "install:browsers:firefox": "playwright install firefox",
    "install:browsers:webkit": "playwright install webkit",
    "dev": "cd examples/showcase && trunk serve",
    "build:examples": "cd examples/showcase && trunk build",
    "clean": "cargo clean && rm -rf dist/ target/"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloud-shuttle/leptos-motion.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/cloud-shuttle/leptos-motion/issues"
  },
  "homepage": "https://github.com/cloud-shuttle/leptos-motion#readme"
}
